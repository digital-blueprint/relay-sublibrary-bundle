{
  "extends": [
    "config:base",
    "group:allNonMajor",
    "schedule:weekends",
    ":automergePatch"
  ],
  "ignorePresets": [":prHourlyLimit2"],
  "rangeStrategy": "update-lockfile",
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "symfony/"
      ],
      "allowedVersions": "<6"
    }
  ]
}