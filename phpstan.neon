includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon

parameters:
    inferPrivatePropertyTypeFromConstructor: true
    level: 3
    paths:
        - src
        - tests
    excludePaths:
        - tests/bootstrap.php
    ignoreErrors:
        - message: '#.*NodeDefinition::children.*#'
          path: ./src/DependencyInjection